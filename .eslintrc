{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/recommended"
  ],
  "env": {
    "browser": true,
    "mocha": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "no-console": 0
  },
  "plugins": [
    "import",
    "react"
  ]
}